{
  "name": "ecommers-backend",
  "version": "1.0.0",
  "description": "Backend para sistema de E-commers con Node.js, Express y Sequelize",
  "main": "server.js",
  "scripts": {
    "start":"node server.js",
    "dev":"nodemon server.js",
    "init-db": "node config/createDatabase.js",
    "reset-db": "node config/resetDatabase.js",
    "sedd": "node -e \"require('./seeders/adminSeeder').runSeeders().then(() => process.exit(0)).catch((err) => { console.error(err):process.exit(1); })\"",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:converage": "jest --coverage"
  },
  "lockfileVersion": 3,
  "requires": true,
  "packages": {}
}
