{
  "name": "ecommers-backend",
  "version": "1.0.0",
  "description": "Backend para sistema de E-commers con Node.js, Express y Sequelize",
  "main": "server.js",
  "scripts": {
    "start":"node server.js",
    "dev":"nodemon server.js",
    "init-db": "node config/createDatabase.js",
    "reset-db": "node config/resetDatabase.js",
    "seed": "node -e \"require('./seeders/adminSeeder').runSeeders().then(() => process.exit(0)).catch((err) => { console.error(err):process.exit(1); })\"",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:converage": "jest --coverage"
  },
  "keywords": [
    "e-commerce",
    "nodejs",
    "express",
    "sequelize",
    "jwt",
    "mysql"    
  ],
  "author":"Larause",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2"
  
},
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.2.2"
  }
}
